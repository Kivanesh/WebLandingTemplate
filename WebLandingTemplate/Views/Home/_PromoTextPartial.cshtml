@model WebLandingTemplate.Models.EnterpriseInfoVM
<div class="container pt90 pb60">
    <div class="row align-items-center">
        <div class="col-lg-5 mb30">
            <div class="progress-label clearfix">
                <span class="progress-title">
                    Jquery
                </span>
                <span class="progress-value float-right">
                    25%
                </span>
            </div><!--label-->
            <div class="progress progress-default">

                <div class="progress-bar" role="progressbar" style="width: 25%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <div class="progress-label clearfix">
                <span class="progress-title">
                    photoshop
                </span>
                <span class="progress-value float-right">
                    50%
                </span>
            </div><!--label-->
            <div class="progress progress-default">
                <div class="progress-bar" role="progressbar" style="width: 50%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <div class="progress-label clearfix">
                <span class="progress-title">
                    HTML5
                </span>
                <span class="progress-value float-right">
                    75%
                </span>
            </div><!--label-->
            <div class="progress progress-default">
                <div class="progress-bar" role="progressbar" style="width: 75%" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <div class="progress-label clearfix">
                <span class="progress-title">
                    CSS3
                </span>
                <span class="progress-value float-right">
                    85%
                </span>
            </div><!--label-->
            <div class="progress progress-default">
                <div class="progress-bar" role="progressbar" style="width: 85%" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
        </div>
        <div class="col-lg-6 ml-auto mb30">
            <h3 class="mb20">Nuestro de Trabajo</h3>
                <p>
                    @Html.DisplayFor(model => model.PromotionalText2)
                </p>
                
            <a href="#" class="btn btn-lg btn-outline-primary"> saber Más</a>
        </div>
    </div>
</div>


